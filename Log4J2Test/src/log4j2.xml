<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appenders>
		<console name="console">
			<patternlayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"></patternlayout>
		</console>
	</appenders>
	<loggers>
		<root level="trace">
			<appenderref ref="console"></appenderref>
		</root>
	</loggers>
</configuration>